    <body onkeydown="return validaBody(event)">
        <% include partials/header %>
        <%if(rev.length>0){%>
        <div class="row">
            <div class="col -md-6 mt-4 text-center">
                <h3 class="card-title">
                    Hay usuarios bloqueados solicitando revisión
                </h3>
              <table class="table table-bordered table-hover table-striped">
                <thead>
                  <tr>
                    <td></td>
                    <td>Foto</td>
                    <td>Nombre De Usuario</td>
                    <td>Contraseña</td>
                    <td>Grupo</td>
                    <td>Boleta</td>
                    <td>Usuario</td>
                    <td>Fecha De Nacimiento</td>
                    <td>El Usuario es Administrador?</td>
                    <td>Acciones</td>
                  </tr>
                </thead>
                <tbody>
                  <% if (rev) { %>
                    <% for(var i = 0; i < rev.length; i++) { %>
                      <tr>
                        <td><%= (i + 1) %></td>
                        <td><img class=".img-fluid img-thumbnail rounded-circle mx-auto mb-2" style="width: 5rem; height: 5rem;" src="<%=rev[i].fot_us%>" alt=""></td>
                        <td><%= rev[i].nom_us %></td>
                        <td><%= rev[i].con_us %></td>
                        <td><%= rev[i].gru_us %></td>
                        <td><%= rev[i].bol_us %></td>
                        <td><%= rev[i].use_us %></td>
                        <td><%= rev[i].eda_us %></td>
                        <td><%var adm = ""; 
                          if(rev[i].adm_us){
                            adm="Si";
                              }else{
                            adm="No";
                              }%><%=adm%></td>
                        <td>
                          <a href="/rev/<%= rev[i].id_us %>" class="btn btn-warning -md-6 mt-4">Revisar</a>
                        </td>
                      </tr>
                    <% } %>
                  <% } %>
                </tbody>
              </table>
            </div>
          </div>
          <%}%>
  <div class="row">
    <div class="col -md-6 mt-4 text-center">
        <h3 class="card-title">
            Bienvenido Administrador <%=user%>
        </h3>
      <table class="table table-bordered table-hover table-striped">
        <thead>
          <tr>
            <td></td>
            <td>Foto</td>
            <td>Nombre De Usuario</td>
            <td>Contraseña</td>
            <td>Grupo</td>
            <td>Boleta</td>
            <td>Usuario</td>
            <td>Fecha De Nacimiento</td>
            <td>El Usuario es Administrador?</td>
            <td>Acciones</td>
          </tr>
        </thead>
        <tbody>
          <% if (data) { %>
            <% for(var i = 0; i < data.length; i++) { %>
              <tr>
                <td><%= (i + 1) %></td>
                <td>
                  <%if(!data[i].adm_us){%>
                    <img class=".img-fluid img-thumbnail rounded-circle mx-auto mb-2" style="width: 10rem; height: 7rem;" src="<%=data[i].fot_us%>" alt="">
                  <%}else{%>
                    <img class=".img-fluid img-thumbnail rounded-circle mx-auto mb-2" style="width: 10rem; height: 7rem;" src="./img/MeetULogo.png" alt="">
                  <%}%>
                </td>
                <td><%= data[i].nom_us %></td>
                <td><%= data[i].con_us %></td>
                <td><%= data[i].gru_us %></td>
                <td><%= data[i].bol_us %></td>
                <td><%= data[i].use_us %></td>
                <td><%= data[i].eda_us %></td>
                <td><%var adm = ""; 
                  if(data[i].adm_us){
                    adm="Si";
                      }else{
                    adm="No";
                      }%><%=adm%></td>
                <td>
                  <%if(!data[i].adm_us){%>
                  <a href="/Stalker/<%=data[i].id_us%>" class="btn btn-info -md-6 mt-4">Información Confidencial de <%= data[i].nom_us %></a>
                  <br>
                  <%console.log(data[i].id_us);%>
                  <a href="/RegactAdmin/<%=data[i].id_us%>" class="btn btn-warning -md-6 mt-4">Modificar</a>
                  <a href="/delete/<%=data[i].id_us%>" class="btn btn-danger -md-6 mt-4">Eliminar</a>
                  <% } %>
                </td>
              </tr>
            <% } %>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="wrapper wrapper--w960">
    <div class="card card-2">
        <div class="card-body">
            <h2 class="title">Información De Registro</h2>
            <form method="POST" action="/Reg/ADMIN" enctype="multipart/form-data">
                <div class="input-group">
                    <input class="input--style-2" type="text" placeholder="Nombre" ondrop="return false" ondrag="return false" ondragend="return false" ondragenter="return false" ondragover="return false" ondragstart="return false" onpaste="return false" name="name" id="nombre" onkeydown="return validaNombre(event)">
                </div>
                <div class="input-group">
                    <input class="input--style-2" type="text" placeholder="Usuario" name="user" id="usuario" ondrop="return false" ondrag="return false" ondragend="return false" ondragenter="return false" ondragover="return false" ondragstart="return false" onpaste="return false" onkeydown="return validaUser(event)">
                </div>
                <div class="input-group">
                    <input class="input--style-2" type="password" placeholder="Contraseña" name="pass" ondrop="return false" ondrag="return false" ondragend="return false" ondragenter="return false" ondragover="return false" ondragstart="return false" onpaste="return false" id="contraseña">
