                <div class="row row-space">
                    <div class="col-4">
                        <div class="input-group">
                            <input class="input--style-2 js-datepicker" type="text" placeholder="Fecha De Nacimiento" name="birthday" ondrop="return false" ondrag="return false" ondragend="return false" ondragenter="return false" ondragover="return false" ondragstart="return false" onpaste="return false" readonly="readonly" id="birth">
